{
  "name": "videojs-concurrence-limiter",
  "version": "0.2.0",
  "description": "A Video.js plugin for concurrency control on played contents",
  "repository": {
    "type": "git",
    "url": "git@github.com:toolbox-tve/videojs-concurrence-limiter.git"
  },
  "bugs": {
    "url": "https://github.com/toolbox-tve/videojs-concurrence-limiter/issues"
  },
  "homepage": "https://github.com/toolbox-tve/videojs-concurrence-limiter",
  "main": "src/js/videojs.concurrence.js",
  "dependencies": {
    "jquery": "^2.2.3",
    "video.js": "^5.9.0"
  },
  "devDependencies": {
    "eslint": "^2.9.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-detect-browsers": "^2.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-qunit": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "qunitjs": "^2.0.0-rc1",
    "sinon": "^1.17.4"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf dist && mkdirp dist",
    "watch": "gulp",
    "build": "gulp build",
    "test": "node_modules/.bin/karma start test/karma.conf.js",
    "test:chrome": "node_modules/.bin/karma start test/karma.conf.js --browsers Chrome",
    "test:firefox": "node_modules/.bin/karma start test/karma.conf.js --browsers Firefox",
    "test:ie": "node_modules/.bin/karma start test/karma.conf.js --browsers IE",
    "test:safari": "node_modules/.bin/karma start test/karma.conf.js --browsers Safari",
    "lint": "node_modules/.bin/eslint src"
  },
  "author": "ToolBox-tve",
  "contributors": [
    "dluna <dluna@tbxnet.com>"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "player",
    "video",
    "videojs",
    "videojs-plugin"
  ]
}
